{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "beberlei/assert": "^3.3",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "league/flysystem": "^2.4",
        "league/flysystem-memory": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "qossmic/deptrac-shim": "^0.18.0",
        "sensio/framework-extra-bundle": "6.2.*",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/intl": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/messenger": "6.0.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/notifier": "6.0.*",
        "symfony/process": "6.0.*",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/string": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/uid": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/web-link": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "6.0.*",
        "twig/extra-bundle": "3.3.*",
        "twig/twig": "3.3.*"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.1",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "6.0.*",
        "symfony/var-dumper": "^5.4",
        "symfony/web-profiler-bundle": "6.0.*",
        "vimeo/psalm": "^4.18"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Enraged\\": "src/Enraged/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Enraged\\": "tests/Enraged/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "project:reset": [
            "@auto-scripts",
            "@database:reset",
            "@fixtures:dev"
        ],
        "cache:clear": [
            "@cache:clear:dev",
            "@cache:clear:test",
            "@cache:clear:prod"
        ],
        "cache:clear:dev": [
            "bin/console cache:clear --env=dev"
        ],
        "cache:clear:test": [
            "bin/console cache:clear --env=test"
        ],
        "cache:clear:prod": [
            "bin/console cache:clear --env=prod"
        ],
        "database:reset": [
            "@database:drop:dev",
            "@database:drop:test",
            "@database:create:dev",
            "@database:create:test",
            "@database:migrate:dev",
            "@database:migrate:test"
        ],
        "database:create:dev": [
            "bin/console doctrine:database:create -n --env=dev"
        ],
        "database:create:test": [
            "bin/console doctrine:database:create -n --env=test"
        ],
        "database:create:prod": [
            "bin/console doctrine:database:create -n --env=prod"
        ],
        "database:drop:dev": [
            "bin/console doctrine:database:drop --if-exists --force -n --env=dev"
        ],
        "database:drop:test": [
            "bin/console doctrine:database:drop --if-exists --force -n --env=test"
        ],
        "database:diff:dev": [
            "bin/console doctrine:migrations:diff -n --env=dev"
        ],
        "database:migration:dev": [
            "bin/console doctrine:migrations:generate -n --env=dev"
        ],
        "database:migrate:dev": [
            "bin/console doctrine:migrations:migrate -n --env=dev"
        ],
        "database:migrate:test": [
            "bin/console doctrine:migrations:migrate -n --env=test"
        ],
        "database:migrate:prod": [
            "bin/console doctrine:migrations:migrate -n --env=prod"
        ],
        "fixtures:dev": [
            "bin/console doctrine:fixtures:load --group=dev"
        ],
        "quality": [
            "@quality:fix",
            "@quality:check",
            "@test"
        ],
        "quality:check": [
            "@quality:cs:check",
            "@quality:stan:check",
            "@quality:psalm:check",
            "@quality:deptrac:check"
        ],
        "quality:fix": [
            "@quality:cs:fix"
        ],
        "quality:cs:check": [
            "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config='.php_cs.php' -vvv --dry-run"
        ],
        "quality:cs:fix": [
            "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config='.php_cs.php'"
        ],
        "quality:stan:check": [
            "vendor/bin/phpstan clear-result-cache",
            "vendor/bin/phpstan analyse -c phpstan.neon src tests"
        ],
        "quality:psalm:check": [
            "vendor/bin/psalm --no-cache"
        ],
        "quality:deptrac:check": [
            "vendor/bin/deptrac analyse"
        ],
        "test": [
            "@quality:check",
            "@test:unit",
            "@test:integration",
            "@test:functional"
        ],
        "test:dev": [
            "@quality",
            "@test:unit",
            "@test:integration",
            "@test:functional"
        ],
        "test:unit": [
            "php ./vendor/bin/phpunit --testsuite Unit"
        ],
        "test:integration": [
            "php ./vendor/bin/phpunit --testsuite Integration"
        ],
        "test:functional": [
            "php ./vendor/bin/phpunit --testsuite Functional"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
